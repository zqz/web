package pages

import "github.com/zqz/web/backend/filedb"

templ Files(db *filedb.FileDB) {
	@layout("ðŸ‘‹ Hello World!") {
		{{ files, _ := db.List(0) }}
		<div>
			for _, i := range files {
				{{ url := "/files/" + i.Slug }}
				<div>
					<a href={ templ.URL(url) }>{ i.Name }</a>
				</div>
			}
		</div>
	}
}
